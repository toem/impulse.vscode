{
	"name": "impulse",
	"displayName": "impulse",
	"description": "impulse is a visualization and analysis workbench which helps engineers to comfortably understand and debug complex semiconductor and multi-core software systems.",
	"publisher": "toem-de",
	"homepage": "https://www.toem.de",
	"author": {
		"name": "Thomas Haber"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"version": "0.3.7",
	"engines": {
		"vscode": "^1.60.0"
	},
	"icon": "images/impulse.png",
	"categories": [
		"Visualization",
		"Data Science",
		"Other"
	],
	"keywords": [
		"impulse",
		"visualization",
		"analysis",
		"workbench",
		"vcd",
		"trace",
		"logging",
		"rt"
	],
	"galleryBanner": {
		"color": "#F2F2F2",
		"theme": "light"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/toem/impulse.vscode.git"
	},
	"bugs": {
		"url": "https://github.com/toem/impulse.vscode/issues"
	},
	"extensionKind": [
		"workspace"
	],
	"activationEvents": [
		"onCustomEditor:de.toem.impulse.editor.records",
		"onCustomEditor:de.toem.impulse.editor.wallet",
		"onView:de.toem.impulse.parts.samples",
		"onView:de.toem.impulse.parts.samples.2",
		"onView:de.toem.impulse.parts.samples.3",
		"onView:de.toem.impulse.parts.samples.4",
		"onView:de.toem.impulse.parts.sample",
		"onFileSystem:impulseFs",
		"onCommand:de.toem.impulse.commands.open-multiple"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "de.toem.impulse.editor.records",
				"displayName": "Signal Viewer",
				"selector": [
					{
						"filenamePattern": "*"
					},
					{
						"filenamePattern": "*.vcd"
					},
					{
						"filenamePattern": "*.recJs"
					},
					{
						"filenamePattern": "*.trace"
					},
					{
						"filenamePattern": "*.recML"
					},
					{
						"filenamePattern": "*.recMZ"
					}
				],
				"priority": "option"
			},
			{
				"viewType": "de.toem.impulse.editor.wallet",
				"displayName": "Wallet Editor",
				"selector": [
					{
						"filenamePattern": "*.walml"
					},
					{
						"filenamePattern": "*.walmz"
					}
				],
				"priority": "default"
			}
		],
		"viewsContainers": {
			"panel": [
				{
					"id": "samples-container",
					"title": "Samples",
					"icon": "$(clear-all)"
				}
			]
		},
		"views": {
			"samples-container": [
				{
					"type": "webview",
					"id": "de.toem.impulse.parts.samples",
					"name": "Sample Table 1"
				},
				{
					"type": "webview",
					"id": "de.toem.impulse.parts.samples.2",
					"name": "Sample Table 2",
					"visibility": "hidden"
				},
				{
					"type": "webview",
					"id": "de.toem.impulse.parts.samples.3",
					"name": "Sample Table 3",
					"visibility": "hidden"
				},
				{
					"type": "webview",
					"id": "de.toem.impulse.parts.samples.4",
					"name": "Sample Table 4",
					"visibility": "hidden"
				},
				{
					"type": "webview",
					"id": "de.toem.impulse.parts.sample",
					"name": "Sample Inspector"
				}
			]
		},
		"commands": [
			{
				"command": "de.toem.impulse.commands.merge",
				"title": "Merge Selected with Signal Viewer",
				"enablement": "",
				"category": "impulse"
			},
			{
				"command": "de.toem.impulse.commands.diff",
				"title": "Compare Selected with Signal Viewer",
				"enablement": "",
				"category": "impulse"
			},
			{
				"command": "de.toem.impulse.commands.preferenceFs.open",
				"title": "Open preference FS folder",
				"enablement": "",
				"category": "impulse"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "listMultiSelection",
					"command": "de.toem.impulse.commands.merge",
					"alt": "",
					"group": "3_compare"
				},
				{
					"when": "listMultiSelection",
					"command": "de.toem.impulse.commands.diff",
					"alt": "",
					"group": "3_compare"
				}
			]
		},
		"files.associations": {
			"*.recJs": "javascript"
		},
		"configuration": [
			{
				"title": "impulse",
				"properties": {
					"impulse.java": {
						"type": "string",
						"default": "java -Xms128m -Xmx2048m",
						"description": "Command line to start the impulse java osgi backend. Changes only take effect after a restart. "
					},
					"impulse.port": {
						"type": "string",
						"default": "0",
						"description": "Communication port for the impulse backend. Changes only take effect after a restart. Select 0 for random port."
					},
					"impulse.xdebugger": {
						"type": "boolean",
						"default": false,
						"description": "Enable java extension debugging."
					},
					"impulse.preferences": {
						"type": "string",
						"default": "User",
						"enum": [
							"User",
							"Workspace"
						],
						"enumDescriptions": [
							"Stores impulse preferences in user area",
							"Stores impulse preferences in workspace area"
						]
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"ws": "^6.2.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2",
		"@types/vscode": "^1.60.0",
		"@types/ws": "^6.0.0"
	},
	"impulseBundles": [
		"impulse/plugins/de.toem.impulse.extension.hdl_3.0.1",
		"impulse/plugins/de.toem.impulse.extension.vp_3.0.1.jar",
		"impulse/plugins/de.toem.impulse.extension.spice_3.0.1.jar",
		"impulse/plugins/de.toem.impulse.extension.log_3.0.1.jar",
		"impulse/plugins/de.toem.impulse.extension.embedded_3.0.1.jar"
	]
}
